package day0207;

/**
 * 객체가 특정 클래스로 부터 생성 된 것인지 비교 > casting
 */
public class UseInstanceof {


		
		public void useInstanceof(UserData ud) {
			if(!(ud instanceof UserData)) {//입력된 ud객체가 UserData클래스로 부터 생성되었는지를 비교
				return;
				
				
				
			}
			
		}
		
		
	
	
	
	
	
	public static void main(String[] args) {

	}//main

}//class

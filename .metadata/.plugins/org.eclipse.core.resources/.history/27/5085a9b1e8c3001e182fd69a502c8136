package day0205;

import java.io.File;

/**
 * 
 * File 클래스를 사용한 파일 접근
 * 
 */
public class UseFile {

	public static void main(String[] args) {
		//1. 생성
		File file = new File("c:/dev/temp/java_read.txt");
		System.out.println(file);
		
		if(!file.exists()) {
			System.out.println("파일이 ㅇㅅㅇ");
			return;
			
		}
	
		System.out.println(file.exists());
	//	System.out.println(file.);

	}//main

}//class

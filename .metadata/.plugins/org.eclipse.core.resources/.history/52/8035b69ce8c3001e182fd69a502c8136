package day0205;

import java.io.File;

/**
 * 
 * File 클래스를 사용한 파일 접근
 * 
 */
public class UseFile {

	public static void main(String[] args) {
		//1. 생성
		File file = new File("C:/dev/temp/java_read.txt");
		System.out.println(file);
		System.out.println(file.exists());
		System.out.println(file.getClass());

	}//main

}//class
